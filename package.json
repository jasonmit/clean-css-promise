    "optimizer",
    "promise",
    "promises",
    "promisified",
    "then",
    "thenable"
  ],
  "dependencies": {
    "array-to-error": "^1.0.0",
    "clean-css": "^3.4.8"
  },
  "devDependencies": {
    "@shinnn/eslint-config-node": "^1.0.1",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.1",
    "istanbul-coveralls": "^1.0.3",
    "tape": "^4.2.2"
  }
}
